{
    "env": {
        "browser": true,
        "jest": true
    },
    "extends": [
        "airbnb",
        "plugin:prettier/recommended"
    ],
    "parser": "babel-eslint",
    "rules": {
        "react/jsx-filename-extension": [
            "error",
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ],
        "react/jsx-one-expression-per-line": "off",
        "no-console": "off",
        "import/no-extraneous-dependencies": "off"
    }
}